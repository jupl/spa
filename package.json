{
  "repository": "git",
  "scripts": {
    "start": "npm install --production && npm run server:prod",
    "test": "npm install && npm run karma:once",
    "bower": "bower install --allow-root",
    "build:dev": "npm run bower && gulp build",
    "build:prod": "NODE_ENV=production npm run build:dev",
    "watch:dev": "npm run bower && gulp watch",
    "watch:prod": "NODE_ENV=production npm run watch:dev",
    "server:dev": "npm run bower && gulp server",
    "server:prod": "NODE_ENV=production npm run server:dev",
    "karma:once": "npm run bower && karma start --no-auto-watch --single-run",
    "karma:watch": "npm run bower && karma start --auto-watch --no-single-run"
  },
  "dependencies": {
    "aliasify": "~1.4.0",
    "debowerify": "~0.5.2",
    "envify": "~1.0.1",
    "uglifyify": "~1.2.2",
    "gulp": "~3.5.2",
    "gulp-autoprefixer": "~0.0.6",
    "gulp-browserify": "~0.4.2",
    "gulp-concat": "~2.1.7",
    "gulp-csso": "~0.2.3",
    "gulp-if": "~0.0.5",
    "gulp-plumber": "~0.5.6",
    "gulp-rimraf": "~0.0.9",
    "gulp-sass": "~0.7.0",
    "bower": "~1.2.8",
    "express": "~3.4.8"
  },
  "devDependencies": {
    "gulp-livereload": "~1.1.1",
    "karma-browserify": "~0.1.0",
    "karma-chai-plugins": "~0.2.0",
    "karma-detect-browsers": "~0.1.2",
    "karma-mocha": "~0.1.1",
    "sugar": "~1.4.1"
  },
  "aliasify": {
    "aliases": {
      "client": "./client"
    }
  }
}
